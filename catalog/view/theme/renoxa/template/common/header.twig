<body>
  <header>

<div class="topbsp  text-center">
  <div class="container">
    <div class="row">
        <div class="col-sm-5 col-xs-12 text-left offtitle"><p>{{ offer_title }}</p></div>
      
      <div class="col-sm-7 col-xs-12 text-right">
        <li class="dropdown inuser">
          <a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown">
            <svg><use xlink:href="#huser"></use></svg>
             <span class="hidden-xs">&nbsp;{% if logged %}{{ text_logout }}{% else %}{{ text_account }}{% endif %}</span> <i class="hidden-xs fa fa-angle-down"></i> 
          </a>
          <ul class="dropdown-menu dropdown-menu-right user-down haccount {% if logged %} alogin {% else %} hlogout {% endif %}">
            {% if logged %}
            <li><a href="{{ account }}"><svg width="16px" height="16px"><use xlink:href="#acprofile"></use></svg>{{ text_account }}</a></li>
            <li><a href="{{ order }}"><svg width="16px" height="16px"><use xlink:href="#acorder"></use></svg>{{ text_order }}</a></li>
            <li><a href="{{ transaction }}"><svg width="16px" height="16px"><use xlink:href="#actranse"></use></svg>{{ text_transaction }}</a></li>
            <li><a href="{{ download }}"><svg width="16px" height="16px"><use xlink:href="#acdownload"></use></svg>{{ text_download }}</a></li>
            <li><a href="{{ logout }}"><svg width="16px" height="16px"><use xlink:href="#aclogout"></use></svg>{{ text_logout }}</a></li>
            {% else %}
            <h5 class="text-left">Hesabnz</h5>
            <h6 class="text-left">Hesaba eriin ve siparileri ynetin</h6>
            <li class="acd"><a href="{{ register }}"><i class="fa fa-user-plus"></i> {{ text_register }}</a></li><li class="acd"><a href="{{ login }}"><i class="fa fa-lock"></i> {{ text_login }}</a></li>
